function search(){
    var search_string = $('#search_string2').val();
    if(search_string){
  	  var search_string = $('#search_string2').val();
 	}
 	else {
 		var search_string = $('#search_string').val();
 		if(search_string){
 			var search_string = $('#search_string').val();
 		}
 		else {
 			search_string = '';
 		}
 	};
    window.location = '//starfrosch.com/hot-100/search/'+encodeURIComponent(search_string.toLowerCase());
    return false;
};
